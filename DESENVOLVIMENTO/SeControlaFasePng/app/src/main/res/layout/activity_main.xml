<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wallpaper_roxo_mobile"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/btnNovaReceitaInicio2"
        android:layout_width="200dp"
        android:layout_height="138dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnNovoGastoInicio2"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/btnnovarenda" />

    <ImageView
        android:id="@+id/btnNovoGastoInicio2"
        android:layout_width="272dp"
        android:layout_height="138dp"
        android:layout_marginTop="484dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/btnnovogasto" />

</android.support.constraint.ConstraintLayout>